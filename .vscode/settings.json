{
  "files.exclude": {
    "**/__pycache__": true
  },
  "autopep8.args": [
    "--max-line-length",
    "120",
    "--experimental"
  ],
  "editor.rulers": [
    120
  ],
  "cSpell.words": [
    "aconvert",
    "ahas",
    "ahash",
    "aiocron",
    "arrowless",
    "changeslog",
    "copyts",
    "daterange",
    "dotenv",
    "finaldir",
    "getpid",
    "gpac",
    "httpx",
    "levelno",
    "libcurl",
    "libx",
    "matroska",
    "mkvtoolsnix",
    "mpegts",
    "msvideo",
    "muxdelay",
    "nodesc",
    "noprogress",
    "postprocessor",
    "preferredcodec",
    "preferredquality",
    "quicktime",
    "tmpfilename",
    "urandom",
    "vcodec",
    "vconvert",
    "writedescription",
    "xerror"
  ],
  "css.styleSheets": [
    "ui/assets/css/*.css"
  ],
  "spellright.language": [
    "en"
  ],
  "spellright.documentTypes": [
    "latex",
    "plaintext"
  ],
}
